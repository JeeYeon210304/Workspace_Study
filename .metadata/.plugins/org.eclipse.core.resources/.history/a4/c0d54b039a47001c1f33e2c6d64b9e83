package JustForCodeTest;
// 코드 막 연습하는 클라스입니다.
public class testClass {
	public static void main(String[] args) {
		
		Gugudan();
		StarTower();
		
	}
	// *별찍기
	public static void StarTower() {
		
		
	}
	// 구구단 5단	
	public static void Gugudan() {
		for(int i = 1; i <= 9; i++){
			System.out.println("5 x " + i + " =  " + 5*i);
		}
				
	}
}
